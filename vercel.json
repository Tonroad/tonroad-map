{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Content-Security-Policy",
          "value": "default-src * data: blob: 'unsafe-inline' 'unsafe-eval'; frame-ancestors *;"
        }
      ]
    }
  ]
}
